class Solution(object):
    def isPalindrome(self, s):
        s = s.lower()
        s1 = []
        for char in s:
            if char.isalnum():
                s1.append(char)
    # built in method reversed creates an iterator, list() turns it into a list
        s2 = list(reversed(s1))
        if s1 == s2:
            return True
        return False

time complexity O(N) space complexity O(N)
